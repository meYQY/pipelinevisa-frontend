# 数据库连接（如果前端直接连接）
# DATABASE_URL=postgresql://user:password@host:port/database

# 第三方服务
# SENTRY_DSN=your-sentry-dsn
# GOOGLE_ANALYTICS_ID=your-ga-id

# 功能开关
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_DEBUG=false